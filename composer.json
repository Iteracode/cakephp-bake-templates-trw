{
    "name": "iteracode/cakephp-bake-templates-trw",
    "description": "A better cake bake generation",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "cakephp/cakephp": "^3.5",
        "cakephp/bake": "^1.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "CakephpBakeTemplatesTrw\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakephpBakeTemplatesTrw\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
